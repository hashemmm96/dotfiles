!Font
URxvt.font: xft:Inconsolata\ for\ Powerline:size=11

!Font spacing
URxvt.letterSpace: -1

!Make urls clickable, choose default browser to open with,
!and choose button that opens link (1 is left click)
URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      firefox
URxvt.matcher.button:   1

!Transparency
URxvt.transparent: false
URxvt.shading: 10

!Scrollbar
URxvt.scrollBar: False

!amount of lines in scrolling history
URxvt.saveLines: 8000
!Cursor blinking
URxvt.cursorBlink: 1
!Colors

urxvt.depth:        32
urxvt.background:   [100]#272822

URxvt*foreground:   #FFFFFF
URxvt*cursorColor:  #FFFFFF

